{
    "name": "osmon-dynamic",
    "displayName": "Osmon Lang",
    "description": "Osmon Dasturlash Tili uchun VSCode kengaytma",
    "version": "0.2.2",
    "publisher": "osmon",
    "repository": {
        "type": "git",
        "url": "https://github.com/osmon-lang/osmon-syntax-highlighter"
    },    
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "icon": "images/icon.png",
    "contributes": {
        "languages": [{
            "id": "osmon",
            "aliases": ["Osmon Lang", "osmon","osm"],
            "extensions": [".osm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "osmon",
            "scopeName": "source.osm",
            "path": "./syntaxes/osm.tmLanguage.json"
        }]
    }
}